#include <stdio.h>

int main(void)
{
    int dayCode, hour;

    if (scanf("%d %d", &dayCode, &hour) != 2)
        return 1;

    if (dayCode >= 1 && dayCode <= 5)
    {
        if (hour >= 8 && hour <= 17)
            printf("System Running (Workday)");
        else
            printf("System Idle (Off-hours)");
    }
    else if (dayCode == 6 || dayCode == 7)
    {
        printf("Weekend Relax Mode");
    }
    else
    {
        printf("Invalid Day Code");
    }

    return 0;
}

