#include <stdio.h>

int main() {
    int score;
    printf("Enter your score: ");   
    if (scanf("%d", &score) != 1)
        return 1;
    char grade;
    if (score >= 80)
        grade = 'A';
    else if (score >= 70)
        grade = 'B';
    else if (score >= 60)
        grade = 'C';
    else if (score >= 50)
        grade = 'D';
    else
        grade = 'F';
    printf("Grade %c", grade);
    return 0;
}
