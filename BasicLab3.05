#include <stdio.h>

typedef struct
{
    int id;         
    float price;      
    int quantity;     
} Order;

int readOrder(Order *o)
{
    if (scanf("%d", &o->id) != 1)
    {
        return 0;
    }

    if (scanf("%f", &o->price) != 1)
    {
        return 0;
    }

    if (scanf("%d", &o->quantity) != 1)
    {
        return 0;
    }

    return 1;
}

float computeOrderCost(Order o)
{
    float baseCost;
    float shippingFee;
    float finalCost;

    shippingFee = 50.0;

    baseCost = o.price * o.quantity;

    finalCost = baseCost + shippingFee;

    if (finalCost >= 500.0)
    {
        finalCost = finalCost * 0.90;
    }

    return finalCost;
}

int main(void)
{
    int totalOrders;
    int index;
    float grandTotal;
    Order current;

    grandTotal = 0.0;

    if (scanf("%d", &totalOrders) != 1)
    {
        printf("Error\n");
        return 0;
    }

    if (totalOrders <= 0)
    {
        printf("Error\n");
        return 0;
    }

    for (index = 0; index < totalOrders; index++)
    {
        if (readOrder(&current) == 0)
        {
            return 1;
        }

        grandTotal = grandTotal + computeOrderCost(current);
    }

    printf("Grand Total: %.2f\n", grandTotal);

    return 0;
}
