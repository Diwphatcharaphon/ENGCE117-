#include <stdio.h>

struct Order
{
    int itemId;
    float price;
    int amount;
};

/* คำนวณราคารวมของ 1 คำสั่งซื้อ */
float computeTotal(float price, int amount)
{
    float result;
    float shipping;

    shipping = 50.0;
    result = price * amount;
    result = result + shipping;

    if (result >= 500.0)
    {
        result = result * 0.90;
    }

    return result;
}

int main(void)
{
    int orderCount;
    int index;
    float grandTotal;
    struct Order currentOrder;

    grandTotal = 0.0;

    if (scanf("%d", &orderCount) != 1)
    {
        return 1;
    }

    if (orderCount <= 0)
    {
        printf("Error\n");
        return 0;
    }

    for (index = 0; index < orderCount; index++)
    {
        if (scanf("%d", &currentOrder.itemId) != 1)
        {
            return 1;
        }

        if (scanf("%f", &currentOrder.price) != 1)
        {
            return 1;
        }

        if (scanf("%d", &currentOrder.amount) != 1)
        {
            return 1;
        }

        grandTotal = grandTotal + computeTotal(
            currentOrder.price,
            currentOrder.amount
        );
    }

    printf("Grand Total: %.2f\n", grandTotal);

    return 0;
}
