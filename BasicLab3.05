#include <stdio.h>

typedef struct {
    int id;
    float unitPrice;
    int qty;
} Order;

/* ฟังก์ชันคำนวณราคาสุทธิ */
float calculateCost(float price, int qty)
{
    float total = (price * qty) + 50.0;

    if (total >= 500.0) {
        total -= total * 0.10;
    }

    return total;
}

int main(void)
{
    int n, i;
    float sum = 0.0;
    Order ord;

    if (scanf("%d", &n) != 1 || n <= 0) {
        printf("Error\n");
        return 0;
    }

    for (i = 0; i < n; i++) {
        if (scanf("%d %f %d", &ord.id, &ord.unitPrice, &ord.qty) != 3) {
            return 1;
        }

        sum += calculateCost(ord.unitPrice, ord.qty);
    }

    printf("Grand Total: %.2f\n", sum);
    return 0;
}
