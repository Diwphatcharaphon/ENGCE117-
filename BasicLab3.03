#include <stdio.h>

struct Student {
    char name[50];
    int studentId;
    float score;
};

int main(void) {
    int N;

    if (scanf("%d", &N) != 1)
        return 1;

    struct Student students[N];
    int idx = 0;

    /* รับข้อมูลนักศึกษา */
    while (idx < N) {
        if (scanf("%d %f %s",
                  &students[idx].studentId,
                  &students[idx].score,
                  students[idx].name) != 3) {
            return 1;
        }
        idx++;
    }

    /* แสดงผล */
    idx = 0;
    while (idx < N) {
        printf("ID: %d, Name: %s, Score: %.2f\n",
               students[idx].studentId,
               students[idx].name,
               students[idx].score);
        idx++;
    }

    return 0;
}

