#include <stdio.h>

int main(void)
{
    int id;
    int count = 0;

    float score;
    float sumScore = 0.0;

    char name[50];

    while (scanf("%d", &id) == 1)
    {
        if (scanf("%f", &score) != 1)
        {
            return 1;
        }

        if (scanf("%s", name) != 1)
        {
            return 1;
        }

        sumScore = sumScore + score;
        count++;
    }

    if (count > 0)
    {
        printf("Average Score: %.2f\n", sumScore / count);
        printf("%d\n", count);
    }

    return 0;
}
