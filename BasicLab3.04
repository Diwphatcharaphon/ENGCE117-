#include <stdio.h>

struct Student
{
    int id;
    float score;
    char name[50];
};

void readStudents(struct Student list[], int total)
{
    int i;

    for (i = 0; i < total; i++)
    {
        if (scanf("%d %f %s",
                  &list[i].id,
                  &list[i].score,
                  list[i].name) != 3)
        {
            return;
        }
    }
}

void countResult(struct Student list[], int total, int *pass, int *fail)
{
    int i;

    *pass = 0;
    *fail = 0;

    for (i = 0; i < total; i++)
    {
        if (list[i].score >= 60.0)
        {
            (*pass)++;
        }
        else
        {
            (*fail)++;
        }
    }
}

int main(void)
{
    int N;
    int passCount;
    int failCount;

    if (scanf("%d", &N) != 1)
    {
        return 1;
    }

    if (N <= 0)
    {
        printf("Error\n");
        return 0;
    }

    struct Student students[N];

    readStudents(students, N);
    countResult(students, N, &passCount, &failCount);

    printf("Pass Count: %d\n", passCount);
    printf("Fail Count: %d\n", failCount);

    return 0;
}
