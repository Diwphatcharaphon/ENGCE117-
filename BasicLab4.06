#include <stdio.h>

#define STUDENTS 5

int main() {
    int scoreList[STUDENTS];
    int total = 0;
    float avg;
    int countAbove = 0;

    for (int i = 0; i < STUDENTS; i++) {
        scanf("%d", &scoreList[i]);
        total += scoreList[i];  
    }

    avg = (float) total / STUDENTS;

    for (int i = 0; i < STUDENTS; i++) {
        if (scoreList[i] > avg) {
            countAbove++;
        }
    }

    printf("\n--- STUDENT SCORE ANALYSIS REPORT ---\n");
    printf("Recorded Scores: ");
    for (int i = 0; i < STUDENTS; i++) {
        printf("%d ", scoreList[i]);
    }
    printf("\n");

    printf("Total Sum: %d\n", total);
    printf("Average Score: %.2f\n", avg);
    printf("Number of Scores Above Average: %d\n", countAbove);

    return 0;
}

