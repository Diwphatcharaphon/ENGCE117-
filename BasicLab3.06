#include <stdio.h>

struct Transaction {
    char description[50];
    float amount;
};

int main(void) {
    int N;
    float income = 0.0, expense = 0.0;

    if (scanf("%d", &N) != 1)
        return 1;

    struct Transaction transactions[N];
    int idx = 0;

    while (idx < N) {
        if (scanf("%f %s",
                  &transactions[idx].amount,
                  transactions[idx].description) != 2) {
            return 1;
        }

        if (transactions[idx].amount >= 0.0)
            income += transactions[idx].amount;
        else
            expense += -transactions[idx].amount;

        idx++;
    }

    printf("Total Income: %.2f\n", income);
    printf("Total Expense: %.2f\n", expense);
    printf("Net Balance: %.2f\n", income - expense);

    return 0;
}

