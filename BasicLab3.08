#include <stdio.h>

struct Campaign
{
    float price;
    int sales;
    float ads;
};

float selectRate(int sales)
{
    float rate;

    rate = 0.10;

    if (sales >= 10)
    {
        rate = 0.15;
    }

    if (sales >= 20)
    {
        rate = 0.20;
    }

    return rate;
}

float calculateNet(struct Campaign c)
{
    float revenue;
    float commission;
    float rate;
    float net;

    revenue = c.price * c.sales;
    rate = selectRate(c.sales);
    commission = revenue * rate;
    net = commission - c.ads;

    return net;
}

int main(void)
{
    int count;
    int index;
    float netResult;
    struct Campaign current;
    char name[50];

    if (scanf("%d", &count) != 1)
    {
        return 1;
    }

    if (count <= 0)
    {
        printf("Error\n");
        return 0;
    }

    for (index = 0; index < count; index++)
    {
        if (scanf("%s", name) != 1)
        {
            return 1;
        }

        if (scanf("%f", &current.price) != 1)
        {
            return 1;
        }

        if (scanf("%d", &current.sales) != 1)
        {
            return 1;
        }

        if (scanf("%f", &current.ads) != 1)
        {
            return 1;
        }

        netResult = calculateNet(current);

        printf("Campaign: %s\n", name);
        printf("Net Result: %.2f\n", netResult);
    }

    return 0;
}
