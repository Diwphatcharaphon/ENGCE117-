#include <stdio.h>

#define MAX 100

struct Student {
    char name[50];
    int studentId;
    float score;
};

int main() {
    int N, i;
    struct Student students[MAX];

    if (scanf("%d", &N) != 1 || N > MAX) {
        return 1;
    }

    for (i = 0; i < N; i++) {
        scanf("%d %f", &students[i].studentId, &students[i].score);
        scanf(" %[^\n]", students[i].name);
    }

    for (i = 0; i < N; i++) {
        printf("ID: %d, Name: %s, Score: %.2f\n",
               students[i].studentId,
               students[i].name,
               students[i].score);
    }

    return 0;
}

