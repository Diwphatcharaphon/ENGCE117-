#include <stdio.h>
void explodeString(char source[], char delimiter, char result[][10], int *wordCount)
{
    int i = 0;
    int j = 0;
    *wordCount = 0;
    while (source[i] != '\0')
    {
        if (source[i] == delimiter)
        {
            result[*wordCount][j] = '\0';
            (*wordCount)++;
            j = 0;
        }
        else
        {
            result[*wordCount][j] = source[i];
            j++;
        }
        i++;
    }
    result[*wordCount][j] = '\0';
    (*wordCount)++;
}
int main()
{
    char output[20][10];
    int count = 0;
    int i;
    explodeString("SA/WAT/DEE/KUB/POMM", '/', output, &count);
    for (i = 0; i < count; i++)
    {
        printf("str2[%d] = %s\n", i, output[i]);
    }
    printf("count = %d\n", count);
    return 0;
}
