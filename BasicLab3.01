#include <stdio.h>

struct Product {
    char name[50];
    int productId;
    float price;
};

int main() {
    struct Product item;

    if (scanf("%d %f", &item.productId, &item.price) != 2) {
        return 1;
    }

    scanf(" %[^\n]", item.name);

    printf("ID: %d, Price: %.2f, Name: %s\n",
           item.productId, item.price, item.name);

    return 0;
}

