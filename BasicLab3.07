#include <stdio.h>

struct SalesRecord
{
    float target;
    float actual;
};

float calculateCommission(struct SalesRecord rec)
{
    float commission;
    float ratio;

    commission = rec.actual * 0.10;
    ratio = rec.actual / rec.target;

    if (ratio >= 1.20)
    {
        commission = commission + 200.0;
    }

    if (ratio >= 1.00 && ratio < 1.20)
    {
        commission = commission + 50.0;
    }

    if (ratio < 0.90)
    {
        commission = commission - 100.0;
    }

    return commission;
}

int main(void)
{
    int count;
    int index;
    float totalCommission;
    struct SalesRecord current;
    char name[50];

    totalCommission = 0.0;

    if (scanf("%d", &count) != 1)
    {
        return 1;
    }

    if (count <= 0)
    {
        printf("Error\n");
        return 0;
    }

    for (index = 0; index < count; index++)
    {
        if (scanf("%f", &current.target) != 1)
        {
            return 1;
        }

        if (scanf("%f", &current.actual) != 1)
        {
            return 1;
        }

        if (scanf("%s", name) != 1)
        {
            return 1;
        }

        totalCommission = totalCommission + calculateCommission(current);
    }

    printf("Grand Total Commission: %.2f\n", totalCommission);

    return 0;
}
