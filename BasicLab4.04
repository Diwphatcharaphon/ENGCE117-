#include <stdio.h>

#define SIZE 5

int main() {
    int data[SIZE];
    int evenTotal = 0;
    int oddTotal = 0;

    for (int idx = 0; idx < SIZE; idx++) {
        scanf("%d", &data[idx]);
    }

    for (int idx = 0; idx < SIZE; idx++) {
        if (data[idx] % 2 == 0) {
            evenTotal++;
        } else {
            oddTotal++;
        }
    }

    printf("\n--- NUMBER CLASSIFICATION REPORT ---\n");
    printf("Recorded Numbers: ");
    for (int idx = 0; idx < SIZE; idx++) {
        printf("%d ", data[idx]);
    }
    printf("\n");

    printf("Total Even Numbers Found: %d\n", evenTotal);
    printf("Total Odd Numbers Found: %d\n", oddTotal);

    return 0;
}

