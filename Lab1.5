#include <stdio.h>

int main() {
    int x;
    int sumEven = 0;
    int sumOdd = 0;

    // อ่านค่าตัวแรก
    if (scanf("%d", &x) != 1)
        return 1;

    // ทำงานจนกว่าจะเจอ 0
    for (;;) {
        if (x == 0)
            break;

        // แยกเลขคู่และเลขคี่
        (x % 2 == 0) ? (sumEven += x) : (sumOdd += x);

        // อ่านค่าถัดไป
        if (scanf("%d", &x) != 1)
            break;
    }

    printf("Even Sum %d\n", sumEven);
    printf("Odd Sum %d\n", sumOdd);

    return 0;
}
