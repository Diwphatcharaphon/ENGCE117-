#include <stdio.h>

int main() {
    int inputNumber = 0;
    int sumEven = 0;
    int sumOdd = 0;
    int readStatus = 0;

    /* รับค่าตัวเลขเริ่มต้น */
    readStatus = scanf("%d", &inputNumber);
    if (readStatus != 1) {
        return 1;
    }

    /* วนซ้ำจนกว่าจะป้อน 0 */
    while (inputNumber != 0) {

        /* เลขคู่หรือเลขคี่ */
        if ((inputNumber % 2) == 0) {
            sumEven = sumEven + inputNumber;
        } else {
            sumOdd = sumOdd + inputNumber;
        }

        /* อ่านค่าถัดไป */
        readStatus = scanf("%d", &inputNumber);
        if (readStatus != 1) {
            return 1;
        }
    }

    /* แสดงผลรวม */
    printf("Even total = %d\n", sumEven);
    printf("Odd total  = %d\n", sumOdd);

    return 0;
}
