#include <stdio.h>

int main(void)
{
    int num1, num2, op, result;

    if (scanf("%d %d %d", &num1, &num2, &op) != 3)
        return 1;

    if (op == 1)
        result = num1 + num2;
    else if (op == 2)
        result = num1 - num2;
    else if (op == 3)
        result = num1 * num2;
    else if (op == 4)
        result = num1 / num2;
    else
    {
        printf("Invalid Operation");
        return 0;
    }

    printf("%d", result);
    return 0;
}

