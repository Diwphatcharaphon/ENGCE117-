#include <stdio.h>
#include <stdlib.h>

char* reverse(char str1[]) {
    int len = 0, i;
    char *str2;

    while (str1[len] != '\0') {
        len++;
    }

    str2 = (char *)malloc((len + 1) * sizeof(char));

    for (i = 0; i < len; i++) {
        str2[i] = str1[len - 1 - i];
    }

    str2[len] = '\0';
    return str2;
}

int main() {
    char text[50];
    char *out;

    fgets(text, 50, stdin);

    for (int i = 0; text[i] != '\0'; i++) {
        if (text[i] == '\n') {
            text[i] = '\0';
            break;
        }
    }

    out = reverse(text);

    printf("%s", out);

    free(out);
    return 0;
}
