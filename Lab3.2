#include <stdio.h>
struct student {
    char name[20];
    int age;
    char sex;
    float gpa;
};
void upgrade(struct student *child) {
    if (child->sex == 'M') {
        child->gpa = child->gpa * 1.10;   // ผู้ชายเพิ่ม 10%
    } else if (child->sex == 'F') {
        child->gpa = child->gpa * 1.20;   // ผู้หญิงเพิ่ม 20%
    }
}
int main() {
    struct student aboy;
    aboy.sex = 'F';
    aboy.gpa = 3.00;
    upgrade(&aboy);   
    printf("%.2f", aboy.gpa);
    return 0;
}
