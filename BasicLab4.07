#include <stdio.h>

#define MAX_SIZE 10

int main() {
    int nums[MAX_SIZE];
    int n, target;
    int freq = 0;

    scanf("%d", &n);
    if (n < 1) n = 1;
    if (n > MAX_SIZE) n = MAX_SIZE;

    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }

    scanf("%d", &target);

    for (int i = 0; i < n; i++) {
        if (nums[i] == target) {
            freq++;
        }
    }

    printf("\n--- FREQUENCY ANALYSIS REPORT ---\n");
    printf("Total elements recorded (N): %d\n", n);

    printf("Recorded Numbers: ");
    for (int i = 0; i < n; i++) {
        printf("%d ", nums[i]);
    }
    printf("\n");

    printf("Search Value: %d\n", target);
    printf("Frequency Count: %d\n", freq);

    return 0;
}

