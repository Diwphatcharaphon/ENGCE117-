#include <stdio.h>

int main(void)
{
    int number;
    int evenSum = 0, oddSum = 0;

    if (scanf("%d", &number) != 1)
        return 1;

    do
    {
        if (number % 2)
            oddSum += number;
        else
            evenSum += number;

        if (scanf("%d", &number) != 1)
            break;

    } while (number != 0);

    printf("Even Sum %d\n", evenSum);
    printf("Odd Sum %d\n", oddSum);

    return 0;
}

